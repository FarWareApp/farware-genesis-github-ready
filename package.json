{
  "name": "farware-genesis",
  "private": true,
  "version": "16.2.0",
  "workspaces": [
    "server",
    "web"
  ],
  "scripts": {
    "dev:server": "npm --workspace server run dev",
    "dev:web": "npm --workspace web run dev",
    "dev:all": "npm-run-all -p dev:server dev:web",
    "build:server": "npm --workspace server run build",
    "build:web": "npm --workspace web run build",
    "start:server": "npm --workspace server start",
    "start:web": "npm --workspace web start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}